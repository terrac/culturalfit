<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel>
		<div class="well ">
			<div class="row">
				<div class="col-md-2">
					<div class="control-group">
						<label for="textinput" class="control-label">Set your desired salary</label>
						<div class="controls">
							<g:ListBox ui:field="salary">
							</g:ListBox>
						</div>
					</div>
				</div>
				<div class="col-md-2">
					<div class="control-group">
						<label for="textinput" class="control-label">Pick a location</label>
						<div class="controls">
							<g:ListBox ui:field="location">
							</g:ListBox>
						</div>
					</div>
				</div>

				<div class="col-md-2">
					<div class="control-group">
						<label for="textinput" class="control-label">Desired vacation</label>
						<div class="controls">
							<g:ListBox ui:field="vacation">
							</g:ListBox>
						</div>
					</div>
				</div>

				<div class="col-2">
					<g:Button ui:field="update" text="Update"></g:Button>
				</div>
			</div>
		</div>


		<g:Button ui:field="publicButton" text="Public Profile"></g:Button>

		<ul class="list-group" ui:field="listGroups">
		</ul>
		
		<g:FlexTable ui:field="listG">
		</g:FlexTable>
	</g:HTMLPanel>
</ui:UiBinder> 