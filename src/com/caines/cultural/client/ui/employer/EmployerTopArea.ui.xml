<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:u="urn:import:com.caines.cultural.client.ui">
	<g:HTMLPanel>
		<div class="container">
			<div class="masthead">
				<h3 class="text-muted">
					<g:Label text="CulturalFit"></g:Label>
				</h3>
				<ul class="nav nav-justified">
					<li class="active">
						<g:Anchor ui:field="findEmployees" href="#">Find employees</g:Anchor>
					</li>
				</ul>
			</div>
			<div class="well well-lg container">
				<div class="row">
					<div class="col-md-2">
						<label class="control-label" for="textinput">Select a radius</label>
						<div class="controls">
							<g:ListBox ui:field="radius">
							</g:ListBox>
						</div>
					</div>

					<div class="col-md-2">
						<label class="control-label" for="textinput">Set a base zip code</label>
						<div class="controls">
							<g:TextBox ui:field="zipCode"></g:TextBox>
						</div>
					</div>

					<div class="col-md-2">
						<label class="control-label" for="textinput">Set a group</label>
						<div class="controls">
							<u:GroupSelect ui:field="groupSelect"></u:GroupSelect>
						</div>
					</div>
				</div>
			</div>
			<ul class="list-group" ui:field="listGroups">
			</ul>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 