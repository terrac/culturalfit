<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<div class="container">
		    <div class="control-group">
			  <div class="controls">
			    <g:SuggestBox ui:field="suggestBox"></g:SuggestBox>    
			    <g:Button ui:field="selectGroup" text="Select Group"></g:Button>
			  </div>
			</div>
		</div>    
	</g:HTMLPanel>
</ui:UiBinder> 